{
  "name": "taste-the-sauce",
  "description": "Cypress testing tutorial using the SauceLabs sample web app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/ramda": "^0.29.3",
    "js-cookie": "^2.2.1",
    "ramda": "^0.29.0",
    "react": "^17.0.2",
    "react-app-polyfill": "^2.0.0",
    "react-burger-menu": "^3.0.6",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^1.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-private-methods": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/register": "^7.13.14",
    "@bahmutov/cypress-code-coverage": "^2.6.1",
    "@cypress/instrument-cra": "^1.4.0",
    "chai-sorted": "^0.2.0",
    "cypress": "^12.17.2",
    "cypress-data-session": "^2.7.0",
    "cypress-map": "^1.21.1",
    "cypress-split": "^1.3.15",
    "cypress-watch-and-reload": "^1.10.6",
    "prettier": "^2.8.3",
    "prop-types": "^15.7.2",
    "start-server-and-test": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "start-18": "react-scripts --openssl-legacy-provider -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "test": "cypress run",
    "cy:open": "cypress open --e2e",
    "dev": "start-test 3001 cy:open",
    "ci": "start-test 3001",
    "lint": "tsc --noEmit --pretty",
    "clean": "rm -rf .nyc_output coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  }
}
